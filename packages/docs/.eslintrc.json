{
  "root": true,
  "parserOptions": {
    "tsconfigRootDir": "./",
    "project": ["./tsconfig.json"],
    "extraFileExtensions": [".vue"]
  },
  "extends": ["custom"],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true,
        "optionalDependencies": false,
        "peerDependencies": false
      }
    ]
  },
    "vue/no-v-model-argument":["error"]
}
