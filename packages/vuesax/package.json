{
  "name": "vuesax-ts",
  "description": "vuesax with full vue3 and typescript support",
  "version": "0.0.11",
  "type": "module",
  "main": "dist/index.umd.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "vue-tsc && eslint src --fix --cache",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run --component"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oil-oil/vuesax-ts.git",
    "directory": "packages/vuesax"
  },
  "dependencies": {
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "nanoid": "^4.0.2",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/node": "^20.1.4",
    "@vitejs/plugin-vue": "^4.1.0",
    "boxicons": "^2.1.4",
    "cypress": "^12.17.2",
    "cypress-real-events": "^1.8.1",
    "eslint-config-custom": "workspace:*",
    "lightningcss": "^1.21.5",
    "sass": "^1.62.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.0.2",
    "vite": "^4.4.8",
    "vite-plugin-dts": "^2.3.0",
    "vue-tsc": "^1.4.2"
  },
  "keywords": [
    "vuesax",
    "component library",
    "ui framework",
    "ui",
    "vue3",
    "typescript"
  ]
}