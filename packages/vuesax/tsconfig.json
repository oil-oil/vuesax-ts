{
  "extends": "tsconfig/vuesax.json",
  "compilerOptions": {
    "types": [
      "cypress",
      "node",
      "cypress-real-events"
    ],
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ],
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "./cypress.d.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}